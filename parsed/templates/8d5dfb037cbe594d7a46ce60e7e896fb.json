{"components/common/cart-preview":"<div class=\"previewCart\">\n    <div class=\"CartMob_topbar\">\n    \t<div class=\"TopBarMenu\">\n    \t\t<h3>My Cart</h3>\n    \t\t<a class=\"closeMenu\" href=\"javascript:void(0)\">ÔÄç<span>Close</span></a>\n    \t</div>\n    </div>\n    {{#if cart.items.length}}\n        <ul class=\"previewCartList\">\n            {{#each (limit cart.items 4)}}\n                <li class=\"previewCartItem\">\n                    <div class=\"previewCartItem-image\">\n                        {{#if type '==' 'GiftCertificate'}}\n                            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\n                        {{else}}\n                            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productthumb_size' (cdn ../../theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        {{/if}}\n                    </div>\n\n                    <div class=\"previewCartItem-content\">\n                        <span class=\"previewCartItem-brand\">        \n                            {{brand.name}}\n                        </span>\n\n                        <h6 class=\"previewCartItem-name\">\n                            <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\">{{name}}</a> \n                        </h6>\n\n                        <span class=\"previewCartItem-price\">\n                            {{#if quantity '>' 0}}\n                                {{quantity}} &times;\n                            {{/if}}\n                            {{#or ../customer (if ../theme_settings.restrict_to_login '!==' true)}}\n                                <span{{#if price_discounted}} class=\"price--discounted\"{{/if}}>{{price.formatted}}</span>\n                                {{#if price_discounted}}\n                                    {{price_discounted.formatted}}\n                                {{/if}}\n                            {{else}}\n                                {{> components/common/login-for-pricing}}\n                            {{/or}}\n                        </span>\n                        <span class=\"previewCartItem-Total-Price\">\n                            <span class=\"lable\">Total:</span>\n                            <span class=\"total-Value\">${{toFixed (multiply quantity price.value) 2}}</span>\n                        </span>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n        <div class=\"previewCartAction\">\n            <div class=\"previewCartAction-checkout\">\n                <ul class=\"total-row\">\n                    <li>\n                        <span class=\"lableTxt\">Shipping</span>\n                        <span class=\"totalTxt\">FREE!</span>\n                    </li>\n                    <li>\n                        <span class=\"lableTxt\">Total</span>\n                        <span class=\"totalTxt\">{{cart.sub_total.formatted}}</span>\n                    </li>\n                </ul>\n                <a href=\"{{urls.cart}}\" class=\"button button--small button--primary\">{{lang 'cart.preview.checkout_now'}}</a>\n            </div>\n            <div class=\"mob_check_img\">\n                <img class=\"Top\" src=\"/content/3-selling-points-cart-model.png\"/>\n                <img class=\"Bottom\" src=\"/content/footer-payment-logos-cart-model.png\"/>\n            </div>\n        </div>\n    {{else}}\n        <div class=\"previewCart-emptyBody\">\n            <img src=\"{{cdn 'webdav:empty-cart.svg'}}\" alt=\"empty-cart\" />\n            <p>There is nothing in your cart.</p>\n            <a href=\"/\">SEARCH TIRES NOW</a>\n            <div class=\"cart-mob-button\">\n                <a href=\"javascript:void(0)\" data-collapsible=\"navPages-mob-BySize\">Shop By Size</a>\n                <a href=\"javascript:void(0)\" data-collapsible=\"navPages-mob-ByVehicle\">Shop By Vehicle</a>\n            </div>\n        </div>\n    {{/if}}\n</div>\n  \n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\n"}
